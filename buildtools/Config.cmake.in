#  Config file for @PROJECTNAME@
#  It defines the following variables
#  @PROJECTNAME_UC_UPPER@_INCLUDE_DIRS - include directories for @PROJECTNAME@
#  @PROJECTNAME_UC_UPPER@_LIBRARIES    - libraries to link against
#  @PROJECTNAME_UC_UPPER@_PREFIX       - prefix where @PROJECTNAME@ is installed
#  @PROJECTNAME_UC_UPPER@_SONAME       - soname of @PROJECTNAME@
#  @PROJECTNAME_UC_UPPER@_SONAME_MAJOR - soname of @PROJECTNAME@
#  @PROJECTNAME_UC_UPPER@_SONAME_MINOR - soname of @PROJECTNAME@
#  @PROJECTNAME_UC_UPPER@_SONAME_PATCH - soname of @PROJECTNAME@

# Export the SONAME for use by plugins
SET( @PROJECTNAME_UC_UPPER@_SONAME "@SONAME@" )
SET( @PROJECTNAME_UC_UPPER@_SONAME_MAJOR "@SONAME_MAJOR@" )
SET( @PROJECTNAME_UC_UPPER@_SONAME_MINOR "@SONAME_MINOR@" )
SET( @PROJECTNAME_UC_UPPER@_SONAME_PATCH "@SONAME_PATCH@" )

# Compute paths
SET( @PROJECTNAME_UC_UPPER@_PREFIX "@PREFIX@" )
GET_FILENAME_COMPONENT( @PROJECTNAME_UC_UPPER@_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH )
IF( EXISTS "${@PROJECTNAME_UC_UPPER@_CMAKE_DIR}/CMakeCache.txt" )
   # In build tree
   INCLUDE( "${@PROJECTNAME_UC_UPPER@_CMAKE_DIR}/@PROJECTNAME_UC@BuildTreeSettings.cmake" )
ELSE()
   SET( @PROJECTNAME_UC_UPPER@_INCLUDE_DIRS "@INSTALL_INCLUDE_DIR_PREFIX@" )
ENDIF()

# Our library dependencies (contains definitions for IMPORTED targets)
INCLUDE( "${@PROJECTNAME_UC_UPPER@_CMAKE_DIR}/@PROJECTNAME_UC@LibraryDepends.cmake" )

# These are IMPORTED targets created by @PROJECTNAME_UC@LibraryDepends.cmake
SET( @PROJECTNAME_UC_UPPER@_LIBRARIES @TARGETLIB@ )

MESSAGE( STATUS "Found @PROJECTNAME_UC@: ${IMPORTED_LOCATION_RELEASE} (Version: @VERSION@)" )
